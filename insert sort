/*

Insert Sort

*/
#include"iostream"
#include"stdlib.h"
#include"time.h"
using namespace std;
void insert_sort(int a[],int n)
{	
	int i;
	int j;
	for(i=2;i<=n;i++)
	{	
		a[0]=a[i];
		j=i-1;	
		while(a[j]>a[0])
		{			
			a[j+1]=a[j];
			j=j-1;
		}
		a[j+1]=a[0];
	}
	cout<<"the sort is:"<<endl;
	for(int s=1;s<=n;s++)
	{
		cout<<a[s]<<" ";
	}
}

int main()
 {	
 	int t;
 	int a[10];
 	srand(time(0));    					
	cout<<"Numembers are:"<<endl;	     
		for(int i=1;i<=10;i++)		// "i =1",because when i=0 the a[0] is set to a temp 	
 	{									
 		t = 1+rand()%100 ;  	   		
 		a[i]=t;
 		cout<<t<<" ";
	 }
	 cout<<endl;
	 insert_sort(a,10);
	 return 0; 																	
 }
